/*78
39 118 59 178 89 268 134 67 202 101 304 152 76 38 19 58 29 88 44 22 11 34 17 52 26 13 40 */
#include<stdio.h>
int main()
{
    int n=27,x;
    scanf("%d",&x);
    
    while(n>0)
    {
        if(x%2==0)
            x/=2;
        else
            x=3*x+1;
            
        printf("%d ",x);
        n--;
    }
}
